{
  "name": "node-docker",
  "version": "1.0.0",
  "description": "project to play with docker, node express nest, and AWS services",
  "main": "src/index.ts",
  "author": "",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development nodemon",
    "serve:tsc": "cross-env NODE_ENV=development npm run serve-run",
    "serve-run": "concurrently \"tsc --watch\" \"nodemon -q ./dist/src/index.js\" --verbose",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node ./dist/src/index.js"
  },
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^8.0.8",
    "@nestjs/core": "^8.0.8",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.8",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nest": "^0.1.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.8",
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "concurrently": "^6.2.2",
    "jest": "^27.2.4",
    "nodemon": "^2.0.13",
    "typescript": "^4.4.3"
  }
}
